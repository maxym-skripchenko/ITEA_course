# Arithmetic Operators

a = 5                    # Create var
b = 2                    # Create var

print(a + b)             # Addition a+b
print(a - b)             # Subtraction a-b
print(a * b)             # Multiplication a*b
print(a / b)             # Division a/b
print(a % b)             # Modulus a % b
print(a ** b)            # Exponent


# Assignment Operators

c = a + b                # Create var
print(c)

c += a                 #  is equivalent to c = c + a
print(c)

c -= a                  #  is equivalent to c = c - a
print(c)

c *= a                 #  is equivalent to c = c * a
print(c)

c /= a                 #  is equivalent to c = c / a
print(c)

c %= a                 #  is equivalent to c = c % a
print(c)

c **= a                 #  is equivalent to c = c ** a
print(c)

c //= a                 #  is equivalent to c = c // a
print(c)
